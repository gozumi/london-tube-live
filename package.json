{
  "name": "london-tube-live",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "ng-build": "ng build --prod",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "docs": "typedoc --experimentalDecorators --ignoreCompilerErrors --out docs/ src/app/index.ts --theme doc-themes/custom",
    "open-docs": "open docs/index.html",
    "build-and-deploy-dev": "npm-run-all -s ng-build sw-precache-dist deploy-dev",
    "deploy-dev": "firebase use develop; firebase deploy",
    "sw-precache-dist": "cp src/manifest.json dist/.; cp -r src/scripts dist/.; cp src/manifest.json dist/.; sw-precache --config=config/sw-precache/sw-precache-config.prod.js --verbose",
    "sw-precache-dev": "sw-precache --config=config/sw-precache/sw-precache-config.dev.js --verbose"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/material": "^2.0.0-beta.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@types/d3": "^4.4.0",
    "angularfire2": "^2.0.0-beta.6",
    "core-js": "^2.4.1",
    "d3": "^4.4.0",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.0.2",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.44",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.24",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-growl-reporter": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-nyan-reporter": "^0.2.4",
    "karma-remap-istanbul": "^0.2.1",
    "karma-spec-reporter": "0.0.26",
    "lodash": "^4.17.3",
    "npm-run-all": "^3.1.2",
    "protractor": "~4.0.13",
    "sw-precache": "^4.2.3",
    "ts-node": "1.2.1",
    "tslint": "^4.0.2",
    "typedoc": "^0.5.1",
    "typedoc-default-themes": "^0.4.1",
    "typescript": "~2.0.3"
  }
}
